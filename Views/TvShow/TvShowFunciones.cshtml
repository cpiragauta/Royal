@model List<CinemaPOS.ModelosPropios.Util.FuncionEncabezado>
@{
    ViewBag.Title = "TvShowFunciones";
    Layout = "~/Views/Inicio/DiseñoTvShow.cshtml";
    int i = 0;
    string html = "";
}

<link href="~/Content/css/TvShowFunciones.min.css" rel="stylesheet" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<link href="~/Content/css/Fuentes.min.css" rel="stylesheet" />
<script src="~/Content/js/jquery-2.2.4.min.js"></script>
<link href="~/Content/css/nifty.min.css" rel="stylesheet" />
<link href="~/Content/css/demo/nifty-demo-icons.min.css" rel="stylesheet">
<link href="~/Content/css/demo/nifty-demo.min.css" rel="stylesheet">
<script src="~/Content/js/nifty.min.js"></script>
<script src="~/Content/js/demo/nifty-demo.min.js"></script>
<link href="~/Content/plugins/TinyCarousel/tinycarousel.css" rel="stylesheet" />
<script type="text/javascript" src="https://code.jquery.com/jquery-latest.min.js"></script>
<script src="~/Content/plugins/TinyCarousel/jquery.tinycarousel.js"></script>
<div class="col-md-12" id="Funciones">
    <div class="" id="slider1">
        <div class="viewport">
            <ul id="comments-list" class="comments-list overview">
            </ul>
        </div>
    </div>
</div>
<script>
    function CargarLista() {

        $.ajax({
            url: "TvString",
            success: function (data) {
                $('#comments-list').append(data);
                $('#slider1').tinycarousel({ interval: true, axis: "y" });
            }
        })
    }
    $(document).ready(function () {
        CargarLista();
    })
</script>
